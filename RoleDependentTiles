<script type="text/javascript">// <![CDATA[
// Look at the different role options on the drop-down: Show only learners the My Required / My Elective widgets
var check = document.getElementById('roleId');
// administrator//
    if (check.options[check.selectedIndex].value == '2000009750') {
		document.getElementById('student').style.display = "block";
		document.getElementById('supervisor').style.display = "block";
		document.getElementById('instructor').style.display = "block";
    }
// domain manager //
	else if (check.options[check.selectedIndex].value == '2000009752') {
		document.getElementById('student').style.display = "none";
		document.getElementById('supervisor').style.display = "none";
		document.getElementById('instructor').style.display = "none";
	}
// instructor //
	else if (check.options[check.selectedIndex].value == '2000009753') {
		document.getElementById('student').style.display = "none";
		document.getElementById('supervisor').style.display = "none";
		document.getElementById('instructor').style.display = "block";
	}
// learner //
	else if (check.options[check.selectedIndex].value == '2000009751') {
		document.getElementById('student').style.display = "block";
		document.getElementById('supervisor').style.display = "none";
		document.getElementById('instructor').style.display = "none";
	}

// supervisor //
	else if (check.options[check.selectedIndex].value == '2000009754') {
		document.getElementById('student').style.display = "none";
		document.getElementById('supervisor').style.display = "block";
		document.getElementById('instructor').style.display = "none";
	}
// ]]></script>
