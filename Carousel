<script type="text/javascript">// <![CDATA[
var classes;
var stuff = "";
var css = "";
var div = document.createElement("div");
div.className = "carousel-custom";
for (i=0; i< Titles.length; i++) {
stuff += '<div class="gallery-cell"><div class="left-box box' + i + '"><a href="' + Nav[i] + '"><div class="icon-circle absolute-center"><i class="fa ' + Icons[i] + ' fa-2x"></i></div></a></div><div class="right-box"><div class="text-box absolute-center">' + Titles[i] + '</div></div></div>'
css += '.box' + i + ' {background-image: url("' + Images[i] + '");}'
div.innerHTML = "<div class='gallery js-flickity' data-flickity-options='{ \"wrapAround\": true, \"freeScroll\": true, \"autoPlay\": 7000 }'>" + stuff + "</div>"
};
$("#wrap").append(div);

// This is where you can change the background image and how it appears. Background image should be changed inside the parens ()
// The text "center center" indicates where the background is positioned. The left option can be top, bottom, or center. The right option can be right, left, or center
// Background-size indicates how the background should appear. Cover will make sure the background image covers all the white space. You can swap this for "contain" or height and width properties.

var background = 'background: url(https://images.unsplash.com/photo-1432821579285-1b649e5b1ce3?dpr=1&auto=format&fit=crop&w=1920&h=1080&q=80&cs=tinysrgb&crop=) center center; background-size: cover;'


var style = document.createElement('style')
style.type = 'text/css'
style.innerHTML =  css + '.left-box a, .left-box a:hover {text-decoration: none !important;}.absolute-center { /* Internet Explorer 10 */ display:-ms-flexbox;-ms-flex-pack:center;-ms-flex-align:center; /* Firefox */ display:-moz-box;-moz-box-pack:center;-moz-box-align:center; /* Safari, Opera, and Chrome */ display:-webkit-box;-webkit-box-pack:center;-webkit-box-align:center;/* W3C */display:box;box-pack:center;box-align:center;}.jumbotron-custom {height: 40vh; width: 100%; ' + background + ' background-repeat: no-repeat;}.logo-circle { height: 140px; width: 140px; border-radius: 100%; overflow: hidden; background-color: white; text-align: center; position: relative; top: calc(40vh - 70px); margin: 0 auto; box-shadow: 0px 1px 5px rgba(0,0,0,0.2); border: 5px solid white;}.logo-circle img {position: absolute; top: 50%; left: 50%; transform: translateX(-50%) translateY(-50%); height: 90%;} .box3, .box6 {background-position: center left !important;}.left-box { display: inline-block; width: 150px; height: 100%; background-position: center, center; background-size: cover; padding: 0; margin: 0; border-top-left-radius: 4px; border-bottom-left-radius: 4px;} .icon-circle { height: 50px; width: 50px; line-height:  64.5px; border-radius: 100%; background-color: white; position: relative; left: 125px; top: 72px; color: rgb(33, 18, 102); box-shadow: 0px 1px 5px rgba(0,0,0,0.2); transition: all ease .5s; pointer-events: all; cursor:pointer;}.left-box a { pointer-events: none; cursor: move; cursor: -webkit-grab; cursor: grab;}.left-box a .icon-circle:hover { color: white; background-color: rgb(33, 18, 102);}.right-box { display: inline-block; float:right; width: 150px; height: 200px; padding: 0; margin: 0; text-align: center;}.text-box { padding: 25px; width: 100%; font-size: 16px; font-weight: 700; height: 100%; color: rgb(80,80,80);}.carousel-custom { margin-top: 100px;}.flickity-viewport {padding-top: 5px; padding-bottom: 5px; height: 210px !important;}.gallery-cell { border-radius: 4px; width: 300px; height: 200px; margin-right: 10px; box-shadow: 0px 1px 5px rgba(0,0,0,0.2); transition: box-shadow ease .35s;}.gallery-cell.is-selected { box-shadow: 0px 1px 8px rgb(33, 18, 102);}.flickity-page-dots {bottom: -22px;}.flickity-page-dots .dot { height: 4px; width: 40px; margin: 0; border-radius: 0;}.flickity-prev-next-button { width: 100px; height: 100px; background: transparent;}.flickity-prev-next-button:hover { background: transparent;}.flickity-prev-next-button .arrow { fill: rgb(33, 18, 102);}.flickity-prev-next-button.no-svg { color: rgb(33, 18, 102);}.flickity-prev-next-button:disabled { display: none;}'

// Options that can be added to the above list of CSS:
// Can add .nav-icon-recordbar-icontext{display:none;} to hide the Close Record button on online resources added to navigation
